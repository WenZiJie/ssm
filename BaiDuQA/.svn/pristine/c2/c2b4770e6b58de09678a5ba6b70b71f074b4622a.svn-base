@charset "UTF-8";
/* bootstrap公共类:用于重写 bootstrap的样式 */


.badge {
	display: inline-block;
	min-width: 10px;
	padding: 3px 7px;
	font-size: 12px;
	font-weight: bold;
	line-height: 1;
	color: #fff;
	text-align: center;
	white-space: nowrap;
	vertical-align: middle;
	background-color: #337AB7;
	border-radius: 10px;
}

.back {
	margin-left: 10px;
}


.has-error .form-control {
	border-color: #a94442;
	 border:2px #a94442 solid;
	-webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
	box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
}

.has-success .form-control {
  border-color: #3c763d;
  border:2px #3c763d solid;
  -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
          box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
}


